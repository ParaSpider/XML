<?xml version="1.0"?>
<map proto = "1.3.2">
<name>Sorrow Between Two Hells</name> <!-- Not sure if you can have | in the map name-->
<version>1.0</version>
<objective>Take control of the hills to earn points. First to 750 points wins!</objective>
<authors>
    <author>varis5hawk</author> <!-- The creator of the map -->
</authors>
<contributors>
    <contributor contribution="Some of the XML">TheLuxeHD</contributor>
</contributors>
<teams>
   <team color="dark red" max="20" max-overfill="25">Red Tean</team>
   <team color="blue" max="20" max-overfill="25">Blue Team</team>
</teams>
<kits>
	<!--Melee Kits-->
	<kit name = "base melee">
	    <boots>diamond boots</boots>
		<leggings>diamond leggings</leggings>
		<chestplate>diamond chestplate</chestplate>
		<helmet>diamond helmet</helmet>
		<potion duration="oo">saturation</potion> <!--I have given a saturation effect because I don't what food to give-->
		<!--Still Need More Info Such as Weapon-->
	</kit>
	<kit name = "Blue Melee" parents = "base melee">
	</kit>
	</kit name = "Red Melee" parents = "base melee">
	</kit>
	<!--Archer Kits-->
	<kit name = "base archer">	
		<potion duration="oo" amplifier="3">speed</potion> <!--Speed 3 forever-->
		<item enchantment = "arrow damage:10;arrow knockback:1">bow</item>  <!--Power 10 , Punch 1 Bow-->
		<item amount = "">arrow</item><!--Specify the amount of arrows you want in the quotations-->
		<potion duration="oo">saturation</potion>   <!--I have given a saturation effect because I don't what food to give-->
	</kit>	
	<kit name  = "Red Archer" parents = "base archer">
		<boots color = "cd0000">leather boots</boots>
		<leggings color = "cd0000">leather leggings</leggings>
		<chestplate>gold chestplate</chestplate>
		<helmet color = "cd0000">leather helmet</helmet>
	</kit>
	<kit name  = "Blue Archer" parents = "base archer">
		<boots color = "0066cc">leather boots</boots>
		<leggings color = "0066cc">leather leggings</leggings>
		<chestplate>gold chestplate</chestplate>
		<helmet color = "0066cc">leather helmet</helmet>
	</kit>	
</kits>
<filters>
	<!--Only Red Team-->
	<filter name = "only-red" parents = "deny-all">
		<allow>
			<team>red</team>
		</allow>
	<filter>
	<!--Only Blue Team-->	
	<filter name = "only-blue" parents = "deny-all">
		<allow>
			<team>blue</team>
		</allow>
	<filter>
</filters>
<regions>
</regions>
<timelock>on</timelock>
</map>
