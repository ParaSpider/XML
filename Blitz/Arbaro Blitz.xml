<map proto="1.3.1">
    <name>Arbaro Blitz</name>
    <version>1.0.0</version>
    <objective>
        Eliminate the other team within 5 minutes!
    </objective>
    <authors>
        <author>carterpaul</author>
        <author>PenguinsCubed</author>
    </authors>
    <contributors>
        <contributor contribution="XML">orky48</contributor>
    </contributors>
    <teams>
        <team color="dark red" max="16">Red Team</team>
        <team color="yellow" max="16">Yellow Team</team>
    </teams>
    <blitz>
        <lives>1</lives>
        <broadcastLives>true</broadcastLives>
        <time>5m</time>
    </blitz>
    <timelock>on</timelock>
    <kits>
        <kit name="spawn">
            <item slot="0">iron axe</item>
            <item slot="1">bow</item>
            <item slot="28" amount="64">arrow</item>
            <item slot="2" amount="64">golden carrot</item>
            <chestplate enchantment="PROTECTION ENVIRONMENTAL">chainmail chestplate</chestplate>
        </kit>
        <kit name="red-spawn" parents="spawn">
            <helmet color="993333">leather helmet</helmet>
            <boots color="993333" enchantment="PROTECTION FALL:1">leather boots</boots>
        </kit>
        <kit name="yellow-spawn" parents="spawn">
            <helmet color="E5E533">leather helmet</helmet>
            <boots color="E5E533" enchantment="PROTECTION FALL:1">leather boots</boots>
        </kit>
    </kits>
    <spawns>
        <spawn team="red" yaw="-90" kit="red-spawn">
            <block>3.5,42,0.5</block>
        </spawn>
        <spawn team="yellow" yaw="90" kit="yellow-spawn">
            <block>109.5,42,0.5</block>
        </spawn>
        <default yaw="180">
            <block>55.5,73,56.5</block>
        </default>
    </spawns>
    <regions>
        <apply block="deny-all">
            <rectangle min="oo" max="oo"/>
        </apply>
    </regions>
</map>
