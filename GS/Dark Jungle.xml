<map proto="1.3.1">
    <name>GS: Dark Jungle</name>
    <version>1.0.0</version>
    <objective>Kill the enemy's squadron!</objective>
    <include src="../classes.xml"/>
    <authors>
        <author>XVenom17</author>
    </authors>
    <contributors>
        <contributior contribution="XML">orky48</contributior>
    </contributors>
    <teams>
        <team color="blue" max="8">Blue Team</team>
        <team color="dark red" max="8">Red Team</team>
    </teams>
    <regions>
        <rectangle name="map" min="-oo,-oo" max="oo,oo"/>
        <apply block="deny-all">
            <region name="map"/>
        </apply>
    </regions>
    <playable>
        <region name="map"/>
    </playable>
    <spawns>
        <spawn team="blue" yaw="-90">
            <cuboid min="-48,38,7" max="-47,38,10"/>
        </spawn>
        <spawn team="red" yaw="90">
            <cuboid min="20,38,7" max="21,38,10"/>
        </spawn>
        <default>
            <block>13.5,48,8.5</block>
        </default>
    </spawns>
</map>
