<map proto="1.3.2">
<name>Glacial Canyon</name>
<version>1.0.0</version>
<objective>
    Break the enemy team's monument!
</objective>
<authors>
    <author>TheMiniMan1</author>
    <author>theaman13</author>
</authors>
<contributors>
    <contributor contribution="XML">orky48</contributor>
</contributors>
<teams>
    <team color="dark red" max="50">Red Team</team>
    <team color="blue" max="50">Blue Team</team>
</teams>
<kits>
    <kit name="spawn">
        <item slot="0">stone sword</item>
        <item slot="1" enchantment="arrow infinite">bow</item>
        <item slot="28">arrow</item>
        <item slot="2">diamond pickaxe</item>
        <item slot="3">stone axe</item>
        <item slot="4" amount="32">wood</item>
        <item slot="5" amount="5">cooked fish</item>
        <chestplate>chainmail chestplate</chestplate>
    </kit>
    <kit name="red-spawn" parents="spawn">
        <helmet color="993333">leather helmet</helmet>
        <boots color="993333">leather boots</boots>
    </kit>
    <kit name="blue-spawn" parents="spawn">
        <helmet color="334CB2">leather helmet</helmet>
        <helmet color="334CB2">leather helmet</helmet>
    </kit>
</kits>
<spawns>
    <spawn team="red" yaw="180" kit="red-spawn">
        <block>-140.5,38,-159.5</block>
    </spawn>
    <spawn team="blue" yaw="0" kit="blue-spawn">
        <block>101.5,38,-209.5</block>
    </spawn>
    <default yaw="180">
        <cuboid min="6.5,68,-280" max="2.5,68,-284.5"/>
    </default>
</spawns>
